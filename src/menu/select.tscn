var start_pos := Vector2.ZERO
var move_pos := Vector2.ZERO

func _ready():
	start_pos = rect_position
	move_pos = start_pos

func _process(delta):
	rect_position = rect_position.move_toward(move_pos, delta * 333)

func move_down():
	move_pos = start_pos + Vector2(90, 180)

func move_back():
	move_pos = start_pos
